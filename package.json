    "prepublishOnly": "git submodule update --init --recursive"
  },
  "peerDependencies": {
    "react-native": "^0.53.3"
  },
  "dependencies": {
    "react-native-events": "^1.0.9"
  },
  "devDependencies": {
    "@types/events": "^1.2.0",
    "@types/react-native": "^0.57.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.1"
  }
}

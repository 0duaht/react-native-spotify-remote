{
  "name": "rn-spotify-remote",
  "repository": {
    "type": "git",
    "url": "https://github.com/cjam/rn-spotify-remote.git"
  },
  "bugs": {
    "url": "https://github.com/cjam/rn-spotify-remote/issues"
  },
  "version": "0.0.1",
  "description": "React Native wrapper around the Spotify Remote SDK",
  "author":{
    "name": "Colter McQuay"
  },
  "nativePackage": true,
  "license": "ISC",
  "keywords": [
    "react-native",
    "spotify",
    "music",
    "audio",
    "remote",    
    "mobile"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn clean && ./node_modules/.bin/tsc",
